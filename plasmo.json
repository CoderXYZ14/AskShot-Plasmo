{
  "extends": "@plasmohq/plasmo/config",
  "popups": {
    "default": "src/popup/index.tsx"
  },
  "contentScripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/contents/draw.ts"],
      "world": "MAIN"
    }
  ],
  "permissions": ["activeTab", "scripting", "storage", "tabs", "identity"],
  "host_permissions": ["http://localhost:3000/*", "http://localhost:3000/*"]
}
